        "cspell:check": "cspell lint ** --show-context",
        "eslint:check": "eslint .",
        "prettier:check": "prettier -c **",
        "stylelint:check": "stylelint src/styles/**.scss",
        "check": "npm-run-all -p cspell:check prettier:check stylelint:check eslint:check",
        "format": "npm-run-all -p cspell prettier stylelint eslint"
