{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard", "eslint:recommended", "plugin:react/recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "react/jsx-no-undef": "off"
    }
}
